<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="200.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <children>
      <CheckBox fx:id="check" mnemonicParsing="false" prefHeight="50.0" prefWidth="300.0" text="Privacy" />
      <HBox prefHeight="50.0" prefWidth="300.0" spacing="10.0">
         <children>
            <Label fx:id="name_lbl" prefHeight="25.0" prefWidth="70.0" text="Name" />
            <TextField fx:id="nameInput" prefHeight="25.0" prefWidth="200.0" promptText="length: 2 ~ 20" />
         </children>
      </HBox>
      <HBox prefHeight="40.0" prefWidth="300.0" spacing="10.0">
         <children>
            <Label fx:id="pw_lbl" prefHeight="25.0" prefWidth="70.0" text="Password" />
            <TextField fx:id="pwInput" prefHeight="25.0" prefWidth="200.0" promptText="length: 4 ~ 30" />
         </children>
      </HBox>
      <Label fx:id="txt_lbl" prefHeight="50.0" prefWidth="300.0">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="btnMake" mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" text="MAKE">
         <VBox.margin>
            <Insets left="110.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>